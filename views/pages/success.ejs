<!-- views/success.ejs -->
<!doctype html>
<html>

<head>
    <title>Google SignIn</title>
   
    <style>
        body {
            padding-top: 70px;
        }
    </style>
</head>

<body>
    <div class="container">
        <div class="jumbotron">
            <h1 class="text-primary  text-center">Profile Information</h1>
            <div class="row">
                <div class="col-sm-6">
                    <div class="well">
                        <p>
                            <strong>Id</strong>: <%= user.id %><br>
                            <strong>Email</strong>: <%= user.emails[0].value %><br>
                            <strong>Name</strong>: <%= user.displayName %>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>

</html>