                                                    <span class="bullets"><img src="/images/list-dark.svg" class="pin-ico float-start me-2"></span>
                                                    <h6 class="gosite_big-txt mb-0 p-0 global-gray"><b>
                                                            <%=post.name%>
                                                        </b></h6>
                                                    <p>
                                                        <%=post.address%>      
                                                    </p>
                                                </div> 
                                            <% }) %> 
                                        <% }%>
                                    </div>
                                    <div class=" process-inner-list row m-0 text-center mt-4 ">
                                        <h5 class="mb-3 text-center"><b>Website Secure</b></h5>
                                        <% if (secure_array.length > 0) { %>
                                            <% secure_array.forEach(function(post){ %>
                                                <% if(post.secure.valid) { %>
                                                    <div class="col-md-4  ps-0 secure-card">
                                                        <div class="process-list p-4">
                                                            <h5 class="text-primary">
                                                                <img src="/images/guard.svg" class="global-ico pe-2" />
                                                                <b> Secure</b>
                                                            </h5>
                                                            <p class="text-center">
                                                                <%=post.name%><br>
                                                                    https://<%=post.url%>
                                                            </p>
                                                        </div>
                                                    </div>
                                                <% } else { %>
                                                    <div class="col-md-4  pe-0 no-secure-card">
                                                        <div class="process-list p-4">
                                                            <h5 class="global-gray"><img src="/images/Cross.svg" class="global-ico pe-2" /><b>Not
                                                                    Secure</b>
                                                            </h5>
                                                            <p class="text-center">
                                                                <%=post.name%><br>
                                                                    http://<%=post.url%>
                                                            </p>
                                                        </div>
                                                    </div>
                                                <% } %>
                                            <% }) %>
                                        <% } %>
                                    </div>
                                    <div class="gosite_page-speed_score mt-4">
                                        <h5 class="mb-4  text-center"><b>Website Page Speed Score</b></h5>
                                        <div class="row px-5 test-progress-bars mb-5">
                                            <div class="col-md-12 test-progress-bars ps-5">
                                                <% if (secure_array.length > 0) { %>
                                                    <% secure_array.forEach(function(post){ %>
                                                        <div class="progress mobile-progress-count">
                                                            <div class="progress-bar text-end" role="progressbar" style="width: 95%;" aria-valuenow="97"
                                                                aria-valuemin="0" aria-valuemax="100">
                                                                <span class="text-start w-auto float-start ps-3">
                                                                    <img src="/images/cellphone-link00.svg" class="global-ico pe-2" />
                                                                    <%=post.name%>
                                                                </span>
                                                                <span class="text-end w-auto float-end">
                                                                    <%=(post.score * 100).toFixed(0)%>%
                                                                </span>
                                                            </div>
                                                        </div>
                                                    <% }) %>
                                                <% } %>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <%- include('../partials/footer'); %>
        <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
        <script src="https://unpkg.com/axios/dist/axios.min.js"></script>

        <script>
            $('#show-more-content').hide();

            $('#show-more').click(function(){
                $('#show-more-content').show();
                $('#show-less').show();
                $('#show-more').hide();
            });

            $('#show-less').click(function(){
                $('#show-more-content').hide();
                $('#show-more').show();
                $(this).hide();
                $('html,body').animate({
                    scrollTop: $(".Placement-table").offset().top},
                    'slow');
            });
        </script>
        <script>
            const total = $("#total").val();
            const brand_search = $("#brand_search").val();
            const discovery_search = $("#discovery_search").val();
            const direct_search = $("#direct_search").val();
            const ctx = document.getElementById('myChart');
            const myChart = new Chart(ctx, {
                type: 'doughnut',
                data: {
                    labels: ['Direct', 'Branded', 'Discovery'],
                    datasets: [{
                        label: '# of Votes',
                        data: [direct_search, brand_search, discovery_search],
                        backgroundColor: [
                            '#c2d6ff',
                            '#3d7eff',
                            '#0357ff'
                        ],
                        hoverOffset: 4
                    }]
                }
            });    
        </script>

        <script>
            const set_score = "<%=scan.set_score%>" / 6;
            const remaining = 100 - set_score;
            const abc = document.getElementById('setChart');
            const setChart = new Chart(abc, {
                type: 'doughnut',
                data: {
                    labels: [],
                    datasets: [{
                        label: '# of Votes',
                        data: [remaining.toFixed(0),set_score.toFixed(0)],
                        backgroundColor: [
                            '#c2d6ff',
                            '#0357ff'
                        ],
                        hoverOffset: 4
                    }]
                }
            });
        </script>
</body>

</html>